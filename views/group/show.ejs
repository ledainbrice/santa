
<br><br>
<div class="container">
	<h1>
		<% if(user.admin){%>
			<i class="fa fa-user-secret" aria-hidden="true"></i>
		<% }else{ %>
			<i class="fa fa-user" aria-hidden="true"></i>
		<% } %>
		<%= user.name %>
	</h1>
	<h3><%= user.firstname %></h3>
	<hr>
	<h3>contact: <%= user.email %></h3>

	<a class="btn btn-medium btn-primary" href="/user/edit/<%= user.id %>">Editer</a>
</div>